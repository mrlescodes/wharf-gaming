{
  "name": "@wharf-gaming/root",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "turbo build",
    "clean": "turbo clean",
    "dev": "turbo dev",
    "format": "pnpm run format:js && pnpm run format:package-json",
    "format:js": "prettier --write \"**/*.{ts,tsx}\"",
    "format:package-json": "sort-package-json  \"package.json\" \"apps/*/package.json\" \"packages/*/package.json\"",
    "lint": "turbo lint"
  },
  "devDependencies": {
    "prettier": "^3.3.3",
    "sort-package-json": "^2.10.1",
    "turbo": "^2.1.2"
  },
  "packageManager": "pnpm@9.10.0",
  "engines": {
    "node": ">=20.x",
    "pnpm": ">=9.x"
  }
}
